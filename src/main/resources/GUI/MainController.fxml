<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
<children>
    <TableView fx:id="patientListView" layoutX="-5.0" layoutY="165.0" prefHeight="255.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="145.0">
    </TableView>
    <MenuBar layoutX="-4.0" layoutY="2.0" prefHeight="27.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <menus>
            <Menu mnemonicParsing="false" text="File">
                <items>
                    <MenuItem fx:id="importFromCSV" mnemonicParsing="false" text="Import from .CSV..." />
                    <MenuItem mnemonicParsing="false" text="Export to CSV ..." />
                    <MenuItem fx:id="exitMenuItem" mnemonicParsing="false" onAction="#exitApp" text="Exit" />
                </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
                <items>
                    <MenuItem mnemonicParsing="false" text="Add new patient" />
                    <MenuItem mnemonicParsing="false" text="Edit selected patient" />
                    <MenuItem mnemonicParsing="false" text="Remove selected patient" />
                </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
                <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                </items>
            </Menu>
        </menus>
    </MenuBar>
    <ImageView fx:id="addPatientImage" fitHeight="102.0" fitWidth="111.0" layoutX="20.0" layoutY="32.0" onMouseClicked="#addPatient" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="30.0">
         <image>
            <Image url="@../Images/addPatient.png" />
         </image></ImageView>
    <ImageView fx:id="editPatientImage" fitHeight="102.0" fitWidth="111.0" layoutX="349.0" layoutY="30.0" onMouseClicked="#editPatient" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="349.0" AnchorPane.rightAnchor="349.0" AnchorPane.topAnchor="30.0">
         <image>
            <Image url="@../Images/editPatient.png" />
         </image></ImageView>
    <ImageView fx:id="removePatientImage" fitHeight="102.0" fitWidth="111.0" layoutX="416.0" layoutY="32.0" onMouseClicked="#removePatient" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="30.0">
         <image>
            <Image url="@../Images/removePatient.png" />
         </image></ImageView>
</children>
</AnchorPane>
